#!/bin/bash

WANTED_VERSION=3.10.11
MAYAN_ENV_NAME=django_keycloak

if [ $(pyenv versions | grep ${WANTED_VERSION} | wc -l) -eq "0" ]; 
then 
    pyenv install ${WANTED_VERSION}
fi

if [ $(pyenv versions | grep '\-\->' | grep ${MAYAN_ENV_NAME} | wc -l) -eq "0" ]; 
then 
    pyenv virtualenv ${WANTED_VERSION} ${MAYAN_ENV_NAME}
fi

pyenv local ${MAYAN_ENV_NAME}
pyenv which pip
pyenv which python
pyenv which python3

pip list

